         */
        async function toggleTrackedTag(tagName, color) {
            const pattern = tagName.toLowerCase().trim();
            const existingIndex = trackedTags.findIndex(t => t.pattern === pattern);

            if (existingIndex >= 0) {
                trackedTags.splice(existingIndex, 1);
            } else {
                trackedTags.push({
                    pattern: pattern,
                    type: 'exact',
                    displayName: tagName,
                    color: color
                });
            }

            await saveTrackedTags();
            renderTagList();
            renderStatistics();
        }


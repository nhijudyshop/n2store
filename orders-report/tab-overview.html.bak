        /* Risk Distribution */
        .discount-risk-section {
            background: #f8fafc;
            border-radius: 12px;
            padding: 20px;
            margin-bottom: 25px;
        }

        .discount-risk-section h4 {
            margin: 0 0 15px 0;
            color: #374151;
            font-size: 16px;
            display: flex;
            align-items: center;
            gap: 8px;
        }

        .risk-distribution {
            margin-bottom: 10px;
        }

        .risk-bar {
            height: 30px;
            background: #e5e7eb;
            border-radius: 15px;
            display: flex;
            overflow: hidden;
            margin-bottom: 15px;
        }

        .risk-segment {
            transition: width 0.5s ease;
        }

        .risk-segment.safe {
            background: linear-gradient(135deg, #10b981, #059669);
        }

        .risk-segment.warning {
            background: linear-gradient(135deg, #f59e0b, #d97706);
        }

        .risk-segment.danger {
            background: linear-gradient(135deg, #ef4444, #dc2626);

        /* ================================================
           DISCOUNT STATS SECTION STYLES
           ================================================ */

        /* Section Container */
        #discountStatsSection {
            background: white;
            border-radius: 15px;
            padding: 25px;
            margin-bottom: 25px;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
        }

        #discountStatsSection.loading {
            opacity: 0.6;
            pointer-events: none;
        }

        #discountStatsSection h2 {
            margin: 0 0 20px 0;
            color: #333;
            font-size: 20px;
            font-weight: 600;
            display: flex;
            align-items: center;
            gap: 10px;
            padding-bottom: 15px;
            border-bottom: 3px solid #f59e0b;
        }

        #discountStatsSection h2 i {
            color: #f59e0b;
        }

        /* Sub-tabs */
        .discount-sub-tabs {
            display: flex;
            gap: 5px;
            margin-bottom: 20px;
            background: #f3f4f6;
            padding: 5px;
            border-radius: 12px;

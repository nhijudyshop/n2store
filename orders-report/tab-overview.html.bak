        }

        .discount-kpi-card.warning {
            border-left-color: #f59e0b;
        }

        .discount-kpi-card.black {
            border-left-color: #1f2937;
        }

        .discount-kpi-card.teal {
            border-left-color: #14b8a6;
        }

        .discount-kpi-card.orange {
            border-left-color: #f97316;
        }

        .discount-kpi-card .kpi-icon {
            font-size: 24px;
            margin-bottom: 10px;
            color: #6b7280;
        }

        .discount-kpi-card.red .kpi-icon {
            color: #ef4444;
        }

        .discount-kpi-card.green .kpi-icon {
            color: #10b981;
        }

        .discount-kpi-card.blue .kpi-icon {
            color: #3b82f6;
        }

        .discount-kpi-card.purple .kpi-icon {
            color: #8b5cf6;
        }

        .discount-kpi-card.warning .kpi-icon {
            color: #f59e0b;
        }

        .discount-kpi-card.black .kpi-icon {
            color: #1f2937;
        }

        .discount-kpi-card.teal .kpi-icon {
            color: #14b8a6;
        }

        .discount-kpi-card.orange .kpi-icon {
            color: #f97316;
        }

        .discount-kpi-card .kpi-value {
            font-size: 24px;
            font-weight: 700;
            color: #1f2937;
            margin-bottom: 5px;

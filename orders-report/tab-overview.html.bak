            { pattern: 'ƒë√£ ƒëi ƒë∆°n g·∫•p', type: 'exact', displayName: 'ƒê√£ ƒêi ƒê∆°n G·∫•p', color: '#10b981' },
            { pattern: 'ok', type: 'startsWith', displayName: 'OK + NV', color: '#22c55e' },
            { pattern: 'qua l·∫•y', type: 'exact', displayName: 'Qua L·∫•y', color: '#3b82f6' },
            { pattern: 'x·ª≠ l√Ω', type: 'startsWith', displayName: 'X·ª≠ L√Ω + NV', color: '#f97316' },
            { pattern: 'ch·ªù live', type: 'exact', displayName: 'Ch·ªù Live', color: '#ec4899' },

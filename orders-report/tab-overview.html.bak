        /**
         * Find GI·ªé TR·ªêNG validation errors
         * Returns orders that violate the rules:
         * 1. Has "gi·ªè tr·ªëng" tag but product count > 0
         * 2. Has product count = 0 but no "gi·ªè tr·ªëng" or "ƒë√£ g·ªôp ko ch·ªët" tag
         */
        function findGioTrongValidationErrors(orders) {
            const invalidOrders = [];
            const gioTrongPatterns = ['gi·ªè tr·ªëng'];
            const gopPatterns = ['ƒë√£ g·ªôp ko ch·ªët', 'ƒë√£ g·ªôp kh√¥ng ch·ªët'];

            orders.forEach(order => {
                const orderTags = parseOrderTags(order.Tags);
                const productCount = order.Details?.length || 0;

                // Check if order has gi·ªè tr·ªëng tag
                const hasGioTrongTag = orderTags.some(tag => {
                    const tagName = (tag.Name || tag.name || '').toLowerCase().trim();
                    return gioTrongPatterns.some(p => tagName.startsWith(p));
                });

                // Check if order has g·ªôp tag
                const hasGopTag = orderTags.some(tag => {
                    const tagName = (tag.Name || tag.name || '').toLowerCase().trim();
                    return gopPatterns.some(p => tagName.startsWith(p));
                });

                // Case 1: Has gi·ªè tr·ªëng tag but has products
                if (hasGioTrongTag && productCount > 0) {
                    invalidOrders.push({
                        order: order,
                        reason: `C√≥ tag "gi·ªè tr·ªëng" nh∆∞ng c√≥ ${productCount} s·∫£n ph·∫©m`
                    });
                }

                // Case 2: No products but missing gi·ªè tr·ªëng or g·ªôp tag
                if (productCount === 0 && !hasGioTrongTag && !hasGopTag) {
                    invalidOrders.push({
                        order: order,
                        reason: 'Kh√¥ng c√≥ s·∫£n ph·∫©m nh∆∞ng thi·∫øu tag "gi·ªè tr·ªëng" ho·∫∑c "ƒë√£ g·ªôp ko ch·ªët"'
                    });
                }
            });

            return invalidOrders;
        }


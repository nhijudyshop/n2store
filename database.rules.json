{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "savedProducts": {
      ".indexOn": [
        "Id",
        "isHidden"
      ]
    },
    "savedProductsMeta": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "displaySettings": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "syncCurrentPage": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "pancake_jwt_token": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "tpos_token": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "chat_products": {
      "shared": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "chat_products_history": {
      "shared": {
        ".indexOn": [
          "timestamp"
        ],
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "order_products": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "order_notes_snapshot": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "productAssignments_history": {
      ".indexOn": [
        "timestamp"
      ],
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "app_version": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}